SELECT team, wins, losses, year,
       CAST(wins AS float) / (wins + losses) AS win_percentage
FROM teams
WHERE year >= 2012
ORDER BY win_percentage DESC
LIMIT 5;
SELECT team, wins, losses, year,
       CAST(wins AS float) / (wins + losses) AS win_percentage
FROM teams
WHERE year >= 2012
ORDER BY win_percentage ASC
LIMIT 5;
